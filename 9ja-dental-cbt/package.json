{
  "name": "9ja-dental-cbt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "wrangler:dev": "pnpx wrangler dev",
    "dev:cf": "pnpx @opennextjs/cloudflare build && wrangler dev",
    "dev:remote": "pnpx @opennextjs/cloudflare build && wrangler dev --remote",
    "build:cf": "pnpx @opennextjs/cloudflare build",
    "deploy:cf": "pnpm build:cf && wrangler deploy",
    "preview:cf": "pnpx @opennextjs/cloudflare preview",
    "deploy": "pnpm build:cf && wrangler deploy",
    "deploy:preview": "pnpm build:cf && wrangler deploy --env preview",
    "deploy:wsl": "pnpx @opennextjs/cloudflare deploy",
    "db:generate": "drizzle-kit generate",
    "db:generate:named": "drizzle-kit generate --name",
    "db:migrate:local": "wrangler d1 migrations apply dental-data --local",
    "db:migrate:preview": "wrangler d1 migrations apply dental-data --env preview",
    "db:migrate:prod": "wrangler d1 migrations apply dental-data --remote",
    "db:studio": "drizzle-kit studio",
    "db:studio:local": "drizzle-kit studio --config=drizzle.local.config.ts",
    "db:studio:remote": "drizzle-kit studio --config=drizzle.config.remote.ts",
    "db:inspect:local": "wrangler d1 execute dental-data --local --command=\"SELECT name FROM sqlite_master WHERE type='table';\"",
    "db:inspect:preview": "wrangler d1 execute dental-data --env preview --command=\"SELECT name FROM sqlite_master WHERE type='table';\"",
    "db:inspect:prod": "wrangler d1 execute dental-data --remote --command=\"SELECT name FROM sqlite_master WHERE type='table';\"",
    "db:reset:local": "wrangler d1 execute dental-data --local --command=\"DROP TABLE IF EXISTS todos;\" && pnpm run db:migrate:local",
    "cf:secret": "npx wrangler secret put",
    "cf-typegen": "pnpm exec wrangler types && pnpm exec wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@opennextjs/cloudflare": "^1.9.0",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "better-auth": "^1.3.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "framer-motion": "^12.23.12",
    "hono": "^4.9.9",
    "lucide-react": "^0.540.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@iconify/react": "^6.0.0",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-sqlite3": "^12.4.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "wrangler": "^4.40.3"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
